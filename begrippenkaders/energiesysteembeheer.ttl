  skos:prefLabel "verbruiksinstallatie"@nl;
  skos:broader :installatie;
  dct:source :netcode-elektriciteit .

:transportverzoek a skos:Concept;
  skos:definition "aanvraag voor het beschikbaar stellen van capaciteit voor het transport van energie"@nl;
  skos:scopeNote "Hieronder wordt verstaan een verzoek om het verhogen van de gecontracteerde transportcapaciteit. Bij een kleinverbruikaansluiting is de gecontracteerde transportcapaciteit gelijk aan de aansluitcapaciteit en is een verhoging van de aansluitcapaciteit een impliciet transportverzoek."@nl;
  skos:inScheme <> ;
  skos:prefLabel "transportverzoek"@nl;
  skos:broader :klantverzoek;
  dct:source :netcode-elektriciteit .

:capaciteitsbeperkingscontract a skos:Concept;
  skos:definition "afspraak om tijdelijk, geheel of gedeeltelijk af te zien van het gebruik van de gecontracteerde en beschikbaar gestelde transportcapaciteit"@nl;
  skos:inScheme <> ;
  skos:prefLabel "capaciteitsbeperkingscontract"@nl;
  skos:altLabel "CBC"@nl;
  skos:broader :klantovereenkomst;
  skos:related :gecontracteerde-transportcapaciteit;
  dct:source :netcode-elektriciteit .

:verbinding a skos:Concept;
  skos:definition "object bestemd om daardoorheen of daarlangs energie te transporteren"@nl;
  skos:inScheme <> ;
  skos:prefLabel "verbinding"@nl;
  skos:broader :netfunctie .

:netuitbreiding a skos:Concept;
  skos:definition "toename in lengte, capaciteit of functionaliteit van een energienet"@nl;
  skos:inScheme <> ;
  skos:prefLabel "netuitbreiding"@nl;
  skos:broader :netaanpassingsopdracht;
  skos:related :energienet .


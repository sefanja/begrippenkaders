  skos:inScheme <> ;
  skos:prefLabel "netvervanging"@nl;
  skos:broader :onderhouds-of-inspectieopdracht;
  skos:related :energienet .

:gecontracteerde-transportcapaciteit a skos:Concept;
  skos:definition "maximale capaciteit die op een aansluiting of groep van aansluitingen mag worden gebruikt"@nl;
  skos:inScheme <> ;
  skos:prefLabel "gecontracteerde transportcapaciteit"@nl;
  skos:hiddenLabel "gecontracteerd transportvermogen"@nl, "GTV"@nl;
  skos:broader :transportcapaciteit;
  skos:related :aansluiting, :aangeslotene;
